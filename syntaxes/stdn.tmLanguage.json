{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.stdn",
	"patterns": [
		{
			"include": "#blockComment"
		},
		{
			"include": "#lineComment"
		},
		{
			"include": "#line"
		},
		{
			"include": "#string"
		},
		{
			"include": "#unit"
		},
		{
			"include": "#comma"
		},
		{
			"include": "#numberHolder"
		},
		{
			"include": "#booleanHolder"
		},
		{
			"include": "#unquotedString"
		},
		{
			"include": "#space"
		},
		{
			"include": "#invalid"
		}
	],
	"repository": {
		"blockComment": {
			"begin": "/\\*",
			"end": "\\*/",
			"name": "comment.block"
		},
		"lineComment": {
			"begin": "//",
			"end": "$",
			"name": "comment.line"
		},
		"invalid": {
			"begin": "(?=.)",
			"end": "$",
			"name": "invalid.illegal"
		},
		"space": {
			"match": "\\s"
		},
		"comma": {
			"match": ","
		},
		"numberHolder": {
			"match": "(?<=(?:^|[',\\[\\]}])\\s*)(?:[+-]?Infinity|NaN|0x[\\dA-Fa-f]+|0o[0-7]+|0b[01]+|[+-]?(?:\\d*\\.?\\d+|\\d+\\.)(?:e[+-]?\\d+)?)(?=\\s*(?:$|[',\\[\\]{}]))",
			"name": "invalid.illegal"
		},
		"booleanHolder": {
			"match": "(?<=(?:^|[',\\[\\]}])\\s*)(?:true|false)(?=\\s*(?:$|[',\\[\\]{}]))",
			"name": "invalid.illegal"
		},
		"unquotedString": {
			"match": "(?<=(?:^|[',\\[\\]}])\\s*)[^\\s',\\[\\]{}][^',\\[\\]{}]*?(?=\\s*(?:$|[',\\[\\]{}]))",
			"name": "string.unquoted"
		},
		"string": {
			"name": "string.quoted.single",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"match": "\\\\[\\\\']",
					"name": "constant.character.escape"
				}
			]
		},
		"kstring": {
			"name": "string.quoted.single.katex",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"match": "\\\\{4}",
					"name": "keyword.control"
				},
				{
					"match": "\\\\{3}(?=[^'\\\\]|$)",
					"name": "keyword.control"
				},
				{
					"match": "\\\\[\\\\']",
					"name": "constant.character.escape"
				},
				{
					"match": "\\\\[A-Za-z]+\\*?",
					"name": "support.function.katex"
				},
				{
					"match": "\\\\[{}#$&^_~]",
					"name": "support.function.katex"
				},
				{
					"match": "[{}#^_~]",
					"name": "meta.template.expression.punctuation"
				},
				{
					"match": "\\$",
					"name": "invalid.illegal"
				},
				{
					"match": "&",
					"name": "keyword.control"
				},
				{
					"match": "\\d+",
					"name": "constant.numeric"
				},
				{
					"match": "%.*$",
					"name": "comment.line"
				}
			]
		},
		"unquotedKString": {
			"match": "(?<=(?:^|[',\\[\\]}])\\s*)[^\\s',\\[\\]{}][^',\\[\\]{}]*?(?=\\s*(?:$|[',\\[\\]{}]))",
			"name": "string.unquoted.katex",
			"captures": {
				"0": {
					"patterns": [
						{
							"match": "\\\\{2}",
							"name": "keyword.control"
						},
						{
							"match": "\\\\[A-Za-z]+\\*?",
							"name": "support.function.katex"
						},
						{
							"match": "\\\\[{}#$&^_~]",
							"name": "support.function.katex"
						},
						{
							"match": "[{}#^_~]",
							"name": "meta.template.expression.punctuation"
						},
						{
							"match": "\\$",
							"name": "invalid.illegal"
						},
						{
							"match": "&",
							"name": "keyword.control"
						},
						{
							"match": "\\d+",
							"name": "constant.numeric"
						},
						{
							"match": "%.*$",
							"name": "comment.line"
						}
					]
				}
			}
		},
		"unit": {
			"begin": "\\{",
			"end": "\\}",
			"patterns": [
				{
					"include": "#blockComment"
				},
				{
					"include": "#lineComment"
				},
				{
					"include": "#value"
				},
				{
					"include": "#key"
				},
				{
					"include": "#comma"
				},
				{
					"include": "#space"
				},
				{
					"include": "#invalid"
				}
			]
		},
		"line": {
			"begin": "\\[",
			"end": "\\]",
			"patterns": [
				{
					"include": "#blockComment"
				},
				{
					"include": "#lineComment"
				},
				{
					"include": "#string"
				},
				{
					"include": "#unit"
				},
				{
					"include": "#comma"
				},
				{
					"include": "#numberHolder"
				},
				{
					"include": "#booleanHolder"
				},
				{
					"include": "#unquotedString"
				},
				{
					"include": "#space"
				},
				{
					"include": "#invalid"
				}
			]
		},
		"kline": {
			"begin": "\\[",
			"end": "\\]",
			"patterns": [
				{
					"include": "#blockComment"
				},
				{
					"include": "#lineComment"
				},
				{
					"include": "#kstring"
				},
				{
					"include": "#unit"
				},
				{
					"include": "#comma"
				},
				{
					"include": "#numberHolder"
				},
				{
					"include": "#booleanHolder"
				},
				{
					"include": "#unquotedKString"
				},
				{
					"include": "#space"
				},
				{
					"include": "#invalid"
				}
			]
		},
		"children": {
			"begin": "\\[",
			"end": "\\]",
			"patterns": [
				{
					"include": "#blockComment"
				},
				{
					"include": "#lineComment"
				},
				{
					"include": "#line"
				},
				{
					"include": "#string"
				},
				{
					"include": "#unit"
				},
				{
					"include": "#comma"
				},
				{
					"include": "#numberHolder"
				},
				{
					"include": "#booleanHolder"
				},
				{
					"include": "#unquotedString"
				},
				{
					"include": "#space"
				},
				{
					"include": "#invalid"
				}
			]
		},
		"kchildren": {
			"begin": "\\[",
			"end": "\\]",
			"patterns": [
				{
					"include": "#blockComment"
				},
				{
					"include": "#lineComment"
				},
				{
					"include": "#kline"
				},
				{
					"include": "#kstring"
				},
				{
					"include": "#unit"
				},
				{
					"include": "#comma"
				},
				{
					"include": "#numberHolder"
				},
				{
					"include": "#booleanHolder"
				},
				{
					"include": "#unquotedKString"
				},
				{
					"include": "#space"
				},
				{
					"include": "#invalid"
				}
			]
		},
		"value": {
			"patterns": [
				{
					"begin": "(?<=(?:^|[',{}])\\s*(?:id|ref-id|[0-9A-Za-z-_]*-href|[0-9A-Za-z-_]*-src)\\s*)'",
					"end": "'",
					"name": "string.quoted.single",
					"contentName": "markup.underline.link",
					"patterns": [
						{
							"match": "\\\\[\\\\']",
							"name": "constant.character.escape"
						}
					]
				},
				{
					"match": "(?<=(?:^|[',{}])\\s*(?:id|ref-id|[0-9A-Za-z-_]*-href|[0-9A-Za-z-_]*-src)\\s+)(?:[+-]?Infinity|NaN|0x[\\dA-Fa-f]+|0o[0-7]+|0b[01]+|[+-]?(?:\\d*\\.?\\d+|\\d+\\.)(?:e[+-]?\\d+)?)(?=\\s*(?:$|[',\\[\\]{}]))",
					"name": "invalid.illegal"
				},
				{
					"match": "(?<=(?:^|[',{}])\\s*(?:id|ref-id|[0-9A-Za-z-_]*-href|[0-9A-Za-z-_]*-src)\\s+)(?:true|false)(?=\\s*(?:$|[',\\[\\]{}]))",
					"name": "invalid.illegal"
				},
				{
					"match": "(?<=(?:^|[',{}])\\s*(?:id|ref-id|[0-9A-Za-z-_]*-href|[0-9A-Za-z-_]*-src)\\s+)([^\\s',\\[\\]{}][^',\\[\\]{}]*?)(?=\\s*(?:$|[',\\[\\]{}]))",
					"captures": {
						"0": {
							"name": "string.unquoted"
						},
						"1": {
							"name": "markup.underline.link"
						}
					}
				},
				{
					"begin": "(?<=[0-9A-Za-z-_]+\\s*)'",
					"end": "'",
					"name": "string.quoted.single",
					"patterns": [
						{
							"match": "\\\\[\\\\']",
							"name": "constant.character.escape"
						}
					]
				},
				{
					"begin": "(?<=[0-9A-Za-z-_]+\\s*)\\{",
					"end": "\\}",
					"patterns": [
						{
							"include": "#kstring"
						},
						{
							"include": "#children"
						},
						{
							"include": "#space"
						},
						{
							"include": "#invalid"
						}
					]
				},
				{
					"match": "(?<=[0-9A-Za-z-_]+\\s+)(?:[+-]?Infinity|NaN|0x[\\dA-Fa-f]+|0o[0-7]+|0b[01]+|[+-]?(?:\\d*\\.?\\d+|\\d+\\.)(?:e[+-]?\\d+)?)(?=\\s*(?:$|[',\\[\\]{}]))",
					"name": "constant.numeric"
				},
				{
					"match": "(?<=[0-9A-Za-z-_]+\\s+)(?:true|false)(?=\\s*(?:$|[',\\[\\]{}]))",
					"name": "constant.language"
				},
				{
					"begin": "(?<=(?:^|[',{}])\\s*(?:katex|align|alignat|aligned|alignedat|array|arraystretch|Bmatrix|bmatrix|cases|CD|darray|dcases|drcases|gather|matrix|pmatrix|rcases|smallmatrix|split|subarray|Vmatrix|vmatrix)\\s*)\\[",
					"end": "\\]",
					"patterns": [
						{
							"include": "#blockComment"
						},
						{
							"include": "#lineComment"
						},
						{
							"include": "#kline"
						},
						{
							"include": "#kstring"
						},
						{
							"include": "#unit"
						},
						{
							"include": "#comma"
						},
						{
							"include": "#numberHolder"
						},
						{
							"include": "#booleanHolder"
						},
						{
							"include": "#unquotedKString"
						},
						{
							"include": "#space"
						},
						{
							"include": "#invalid"
						}
					]
				},
				{
					"begin": "(?<=(?:^|[',{}])\\s*(?:heading|h1|h2|h3|h4|h5|h6)\\s*)\\[",
					"end": "\\]",
					"name": "markup.heading",
					"patterns": [
						{
							"include": "#blockComment"
						},
						{
							"include": "#lineComment"
						},
						{
							"include": "#line"
						},
						{
							"include": "#string"
						},
						{
							"include": "#unit"
						},
						{
							"include": "#comma"
						},
						{
							"include": "#numberHolder"
						},
						{
							"include": "#booleanHolder"
						},
						{
							"include": "#unquotedString"
						},
						{
							"include": "#space"
						},
						{
							"include": "#invalid"
						}
					]
				},
				{
					"begin": "(?<=[0-9A-Za-z-_]+\\s*)\\[",
					"end": "\\]",
					"patterns": [
						{
							"include": "#blockComment"
						},
						{
							"include": "#lineComment"
						},
						{
							"include": "#line"
						},
						{
							"include": "#string"
						},
						{
							"include": "#unit"
						},
						{
							"include": "#comma"
						},
						{
							"include": "#numberHolder"
						},
						{
							"include": "#booleanHolder"
						},
						{
							"include": "#unquotedString"
						},
						{
							"include": "#space"
						},
						{
							"include": "#invalid"
						}
					]
				},
				{
					"match": "(?<=[0-9A-Za-z-_]+\\s+)[^\\s',\\[\\]{}][^',\\[\\]{}]*?(?=\\s*(?:$|[',\\[\\]{}]))",
					"name": "string.unquoted"
				},
				{
					"include": "#kstring"
				},
				{
					"include": "#kchildren"
				}
			]
		},
		"key": {
			"patterns": [
				{
					"match": "(?<=(?:^|[',{}])\\s*)[0-9A-Za-z-_]+(?=\\s*\\[)",
					"name": "entity.name.type"
				},
				{
					"match": "(?<=(?:^|[',{}])\\s*)[0-9A-Za-z-_]+(?=\\s*[\\s',\\[{}])",
					"name": "entity.other.attribute-name"
				}
			]
		}
	}
}